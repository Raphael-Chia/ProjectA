pipeline {
  agent any
  
  stages{
    stage('Build'){
      steps{
        echo 'Executing Build in dev'
      }
    }
    
    stage('Test'){
      steps{
        echo 'Conducting Test in dev'    
      }
    }
    
    stage('Deploy'){
      steps{
        echo 'Deploying to SIT...'
      }
    }
    
  }
}
